#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/bin/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules:/Users/shilianwang/Documents/project/blog/node_modules:/Users/shilianwang/Documents/project/node_modules:/Users/shilianwang/Documents/node_modules:/Users/shilianwang/node_modules:/Users/node_modules:/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/hexo-renderer-stylus@2.0.1/node_modules/hexo-renderer-stylus/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/hexo-renderer-stylus@2.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/bin/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/stylus@0.54.8/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules:/Users/shilianwang/Documents/project/blog/node_modules:/Users/shilianwang/Documents/project/node_modules:/Users/shilianwang/Documents/node_modules:/Users/shilianwang/node_modules:/Users/node_modules:/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/hexo-renderer-stylus@2.0.1/node_modules/hexo-renderer-stylus/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/hexo-renderer-stylus@2.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
fi
