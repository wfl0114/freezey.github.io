#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules:/Users/shilianwang/Documents/project/blog/node_modules:/Users/shilianwang/Documents/project/node_modules:/Users/shilianwang/Documents/node_modules:/Users/shilianwang/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules/.pnpm/node_modules:/Users/shilianwang/Documents/project/blog/freezey.github.io/node_modules:/Users/shilianwang/Documents/project/blog/node_modules:/Users/shilianwang/Documents/project/node_modules:/Users/shilianwang/Documents/node_modules:/Users/shilianwang/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/marked" "$@"
else
  exec node  "$basedir/../../bin/marked" "$@"
fi
